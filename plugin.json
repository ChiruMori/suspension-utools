{
	"pluginName": "悬浮",
	"description": "悬浮你的图片",
	"author": "qinyongliang",
	"homepage": "https://github.com/qinyongliang/",
	"preload": "preload.js",
	"version": "1.1.0",
	"logo": "icon.png",
	"features": [
		{
			"code": "suspend",
			"cmds": [
				"截图并悬浮",
				{
					"type": "img",
					"label": "悬浮"
				},
				{
					"type": "files",
					"label": "悬浮图片",
					"fileType": "file",
					"match": "/.(?:bmp|gif|heic|jpeg|jpg|jpe|png|svg|webp|ico)$/i",
					"maxLength": 9
				}
			]
		},
		{
			"code": "suspend-base64",
			"cmds": [
				{
					"type": "regex",
					"label": "悬浮图片",
					"match": "/^data:image\/(bmp|gif|heic|jpeg|jpeg|jpeg|png|svg\\+xml|webp|x-icon);base64,/",
					"minLength":100
				}
			]
		}
	]
}